generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = `postgres://${process.env.POSTGRES_USER}:${process.env.POSTGRES_PASSWORD}@${process.env.DB_HOST}:5432/${process.env.POSTGRES_DB_DEV}`
}
