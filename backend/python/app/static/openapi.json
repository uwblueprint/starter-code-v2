        ],
        "parameters": [
          {
            "in": "path",
            "name": "userid",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The user ID"
          }
        ],
        "tags": [
          "Auth"
        ],
        "description": "Logout as user",
        "responses": {
          "204": {
            "description": "Revokes all of the specified user's refresh tokens"
          },
          "401": {
            "description": "Unauthorized"
          },
          "500": {
            "description": "Invalid request"
          }
        }
      }
    },
